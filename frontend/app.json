{
  "expo": {
    "name": "StudyMon",
    "slug": "studymon",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/studymon-logo.png",
    "scheme": "studymon",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/studymon-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.calvin4370.studymon"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/studymon-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/studymon-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6db5befc-be3e-45a4-8652-5f1a6ae449b5"
      }
    }
  }
}
